<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_cloud_phone_call_form" model="ir.ui.view">
            <field name="name">cloud.phone.call.view.form</field>
            <field name="model">cloud.phone.call</field>
            <field name="arch" type="xml">
                <form string="Call">
                    <group>
                        <field name="connector_id"/>
                        <field name="tel" invisible="1"/>
                        <field name="compute_tel"/>
                        <field name="number_id"/>
                        <field name="type"/>
                        <field name="time"/>
                        <field name="rec_duration"/>
                        <field name="call_duration"/>
                        <field name="filename"/>
                    </group>
                    <group>
                        <field name="external_id"/>
                        <field name="ir_attachment_id" widget="attachment_many2one_audio_widget"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="view_cloud_phone_call_tree" model="ir.ui.view">
            <field name="name">cloud.phone.call.view.tree</field>
            <field name="model">cloud.phone.call</field>
            <field name="arch" type="xml">
                <tree string="Calls">
                    <field name="external_id"/>
                    <field name="tel" invisible="1"/>
                    <field name="compute_tel"/>
                    <field name="number_id"/>
                    <field name="type"/>
                    <field name="time"/>
                    <field name="rec_duration"/>
                    <field name="ir_attachment_id" widget="attachment_audio_widget"/>
                </tree>
            </field>
        </record>

        <record id="action_cloud_phone_call" model="ir.actions.act_window">
            <field name="name">Calls</field>
            <field name="res_model">cloud.phone.call</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!-- <record id="action_cloud_phone_mycall" model="ir.actions.act_window">
