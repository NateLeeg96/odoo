<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_cloud_phone_event_form" model="ir.ui.view">
            <field name="name">cloud.phone.event.view.form</field>
            <field name="model">cloud.phone.event</field>
            <field name="arch" type="xml">
                <form string="Event">
                    <group>
                        <field name="create_date"/>
                        <field name="connector_id"/>
                        <field name="type"/>
                        <field name="call_from"/>
                        <field name="call_to"/>
                        <field name="line_number"/>
                        <field name="disconnect_reason"/>
                        <field name="body"/>
                        <field name="params"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="view_cloud_phone_event_tree" model="ir.ui.view">
            <field name="name">cloud.phone.event.view.tree</field>
            <field name="model">cloud.phone.event</field>
            <field name="arch" type="xml">
                <tree string="Events">
                        <field name="create_date"/>
                        <field name="connector_id"/>
                        <field name="type"/>
                        <field name="call_from"/>
                        <field name="call_to"/>
                        <field name="line_number"/>
                        <field name="disconnect_reason"/>
                        <field name="params"/>
                </tree>
            </field>
        </record>

        <record id="action_cloud_phone_event" model="ir.actions.act_window">
            <field name="name">Events</field>
            <field name="res_model">cloud.phone.event</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>